#wrapper_homepage p.case_desc {padding: 10px 0;}
#wrapper_homepage p.case_desc_last {padding: 0 0 20px;}
#wrapper_homepage a:hover {color:#F00000}

/* Grid setup ---------------------------------------- */
.column-row			{margin: 0 0 80px 0;}
.column-row-alt		{margin: 0 0 30px 0;}
.column-row-sec		{margin: 0 0 18px;clear: both;display: inline-block;float: left;}
.row				{float:left; margin-right:10px; padding-right:10px}
.row_a				{float:left; margin-right:0px; padding-right:10px}
.last-row			{margin-right:0px !important; }
.column, .columns 	{float:left; display: inline; margin: 0 0 0 10px;}
.first_col 			{margin-left:0px !important; }
/* Grid widths --------------------------------------- */
.first				{width: 142px }
.one 				{width: 132px }
.one-and-half		{width: 248px }
.two 				{width: 284px }
.single_archive .two {width: 280px } 
.two_b 				{width: 279px;margin:0}
